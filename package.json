    "scripts": {
        "start": "react-scripts start",
        "storybook": "storybook dev -p 6006 -s public",
        "build-storybook": "storybook build -s public",
        "build": "rmdir dist /s /q && mkdir dist && babel src/lib -d dist --copy-files --extensions \".ts,.tsx\" --no-copy-ignored && tsc -p tsconfig.module.json",
        "build:mac": "rm -r dist && mkdir dist && babel src/lib -d dist --copy-files --extensions \".ts,.tsx\" --no-copy-ignored && tsc -p tsconfig.module.json",
        "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
        "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
        "tsc-build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
